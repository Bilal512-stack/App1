{
  "expo": {
    "name": "client-app1",
    "slug": "client-app1",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mta.png",
        "backgroundColor": "#FFFFFF",
        "backgroundImage": "",
        "monochromeImage": ""
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mta.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mta.png",
          "resizeMode": "cover",
          "backgroundColor": "#FFFFFF",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Outfit-bold.ttf",
            "./assets/fonts/Outfit-Regular.ttf",
            "./assets/fonts/Outfit-SemiBold.ttf"
          ]
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "image": "./assets/images/mta.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "opencageApiKey": "a02dfb9bc224455ea4a328978eba5a78"
    }
  }
}